datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

generator client {
    provider = "prisma-client-js"
}

model Student {
    id         String   @id @default(cuid())
    name       String
    cohort     String
    courses    String[]
    dateJoined DateTime @default(now())
    lastLogin  DateTime @updatedAt
    status     String
}
